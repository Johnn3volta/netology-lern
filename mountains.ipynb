    "%%time\n",
    "import pandas as pd\n",
    "\n",
    "mountains = pd.read_csv('Mountains.csv')\n",
    "\n",
    "mountData = mountains.groupby( by = ['Rank']).agg( [min, max] ).reset_index()\n",
    "mountData['mount'] = mountData['Height (m)']['max'] - mountData['Height (m)']['min']\n",
    "\n",
    "MN = mountData['mount'].mean()\n"
